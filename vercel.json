{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {
        "extensions": [".html"]
      }
    },
    {
      "src": "client/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "styles/**/*.css",
      "use": "@vercel/static-build"
    }
  ]
}
